<!DOCTYPE html>
<html>
<head>
  <title>Harvest</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'application', media: 'all','data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= render 'layouts/shim' %>
</head>

<body>
  <%= render 'layouts/header' %>
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <%= flash_messages %>
      </div>
    </div>
    <%= yield %>
  </div>
</body>
</html>
